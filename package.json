{
  "dependencies": {
    "@maptiler/sdk": "3.11.0",
    "@mdi/js": "7.4.47",
    "@types/geojson": "7946.0.16",
    "@watergis/maplibre-gl-export": "4.1.1",
    "browserslist-to-esbuild": "2.1.1",
    "esbuild": "0.27.2",
    "hono": "4.11.7",
    "maplibre-gl": "5.17.0",
    "simplify-js": "1.2.4"
  },
  "scripts": {
    "build": "deno run --allow-env --allow-read --allow-run --allow-write=www npm:esbuild --bundle --minify --outfile=www/index.js --sourcemap src/map.ts",
    "start": "deno serve --allow-env --allow-net --allow-read=data,www --allow-write=data/trackers.json main.ts",
    "dev": "deno task dev:server & deno task dev:client",
    "dev:client": "deno run --allow-env --allow-read --allow-run --allow-write=www npm:esbuild --bundle --outfile=www/index.js --sourcemap --watch src/map.ts",
    "dev:server": "deno serve --allow-env --allow-net --allow-read=data,www --allow-write=data/trackers.json --env-file --watch main.ts",
    "test": "deno test --allow-env"
  }
}
